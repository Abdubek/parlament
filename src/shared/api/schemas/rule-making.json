  "components": {
    "schemas": {
      "BillDocumentDto": {
        "type": "object",
        "properties": {
          "documentId": {
            "type": "string",
            "description": "Уникальный идентификатор документа",
            "format": "uuid",
            "readOnly": true
          },
          "fileId": {
            "type": "string",
            "description": "Уникальный идентификатор файла",
            "format": "uuid",
            "readOnly": true
          },
          "folderId": {
            "type": "string",
            "description": "Уникальный идентификатор каталога",
            "format": "uuid",
            "readOnly": true
          },
          "language": {
            "type": "string",
            "description": "язык документа",
            "enum": [
              "RU",
              "KZ",
              "EN",
              "OTHER"
            ]
          },
          "pageCount": {
            "type": "integer",
            "description": "Количество листов",
            "format": "int32"
          },
          "documentType": {
            "$ref": "#/components/schemas/EnumTypeDto"
          },
          "questionType": {
            "$ref": "#/components/schemas/EnumTypeDto"
          }
        },
        "description": "Документ"
      },
      "EnumTypeDto": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "description": "Код"
          },
          "nameRu": {
            "type": "string",
            "description": "Название на русском языке",
            "readOnly": true
          },
          "nameKz": {
            "type": "string",
            "description": "Название на казахском языке",
            "readOnly": true
          }
        },
        "description": "Тип (перечисление)"
      },
      "BillDepartmentDto": {
        "type": "object",
        "properties": {
          "department": {
            "$ref": "#/components/schemas/DepartmentDto"
          },
          "departmentType": {
            "type": "string",
            "description": "Тип департамента законопроекта",
            "enum": [
              "GOVERNMENT_AGENCY",
              "RESPONSIBLE_DEPARTMENT"
            ]
          }
        },
        "description": "Департаменты законопроекта"
      },
      "BillDto": {
        "required": [
          "processInstanceId",
          "taskId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Уникальный идентификатор",
            "format": "uuid",
            "readOnly": true
          },
          "folderId": {
            "type": "string",
            "description": "Уникальный идентификатор каталога",
            "format": "uuid",
            "readOnly": true
          },
          "taskId": {
            "type": "string",
            "description": "Идентификатор задачи, связанной с законопроектом.",
            "example": "test-task-0000-0000-0000-000000000001"
          },
          "processInstanceId": {
            "type": "string",
            "description": "Идентификатор экземпляра процесса, связанного с законопроектом.",
            "example": "test-proc-0000-0000-0000-000000000001"
          },
          "nameRu": {
            "type": "string",
            "description": "Наименование законопроекта на русском языке"
          },
          "nameKz": {
            "type": "string",
            "description": "Наименование законопроекта на казахском языке"
          },
          "number": {
            "type": "string",
            "description": "Входящий номер"
          },
          "isMain": {
            "type": "boolean",
            "description": "Основной законопроект"
          },
          "relatedLegalActs": {
            "type": "integer",
            "description": "Связанные НПА",
            "format": "int64"
          },
          "billType": {
            "type": "string",
            "description": "Тип законодательного акта",
            "enum": [
              "LAW",
              "CODE",
              "CONSTITUTIONAL",
              "RESOLUTION"
            ]
          },
          "billStatus": {
            "type": "string",
            "description": "Статус законодательного акта",
            "enum": [
              "NEW",
              "IN_PROGRESS",
              "CLOSED"
            ]
          },
          "billCategory": {
            "type": "string",
            "description": "Вид законодательно акта",
            "enum": [
              "GOVERNMENTAL",
              "DEPUTY",
              "PRESIDENTIAL"
            ]
          },
          "revisionType": {
            "type": "string",
            "description": "Вид редакции проекта",
            "enum": [
              "NEW_EDITION",
              "AMENDMENTS"
            ]
          },
          "session": {
            "$ref": "#/components/schemas/SessionDto"
          },
          "histories": {
            "type": "array",
            "description": "История событий",
            "items": {
              "$ref": "#/components/schemas/BillHistoryDto"
            }
          },
          "documents": {
            "type": "array",
            "description": "Документы законопроекта",
            "readOnly": true,
            "items": {
              "$ref": "#/components/schemas/BillDocumentDto"
            }
          },
          "members": {
            "type": "array",
            "description": "Участники законопроекта",
            "items": {
              "$ref": "#/components/schemas/BillMembersDto"
