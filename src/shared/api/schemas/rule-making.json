    "/bill": {
      "get": {
        "tags": [
          "Bill API"
        ],
        "summary": "Список законопроектов",
        "description": "Возвращает список всех доступных законопроектов",
        "operationId": "getAllBills",
        "parameters": [
          {
            "name": "session",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "department",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "date",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date"
            }
          },
          {
            "name": "status",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": [
                "INCOMING",
                "INITIATED",
                "REJECTED",
                "WITHDRAWN",
                "ON_REVIEW",
                "ADOPTED_BY_MAZHILIS",
                "FOR_APPROVAL",
                "RETURNED_BY_SENATE",
                "ADOPTED_ON_JOINT_SESSION",
                "ADOPTED_BY_SENATE",
                "SENT_TO_SENATE",
                "PRESIDENT_OBJECTIONS",
                "SENT_TO_PRESIDENT",
                "SIGNED_BY_PRESIDENT"
              ]
            }
          },
          {
            "name": "category",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": [
                "GOVERNMENTAL",
                "DEPUTY",
                "PRESIDENTIAL"
              ]
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "Zero-based page index (0..N)",
            "required": false,
            "schema": {
              "minimum": 0,
              "type": "integer",
              "default": 0
            }
          },
          {
            "name": "size",
            "in": "query",
            "description": "The size of the page to be returned",
            "required": false,
            "schema": {
              "minimum": 1,
              "type": "integer",
              "default": 10
            }
          },
          {
            "name": "sort",
            "in": "query",
            "description": "Sorting criteria in the format: property,(asc|desc). Default sort order is ascending. Multiple sort criteria are supported.",
            "required": false,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Успешный ответ",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EnumTypeDto"
                }
              }
            }
